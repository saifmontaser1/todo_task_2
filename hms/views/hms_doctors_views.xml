<odoo>
    <data>
        <record id="doctors_list_view" model="ir.ui.view">
            <field name="name">doctors list view</field>
            <field name="model">hms.doctors</field>
            <field name="arch" type="xml">
                <list>

                        <field name="first_name"/>
                        <field name="last_name"/>
                        <field name="image" widget="image"/>

                </list>
            </field>
        </record>

        <record id="doctors_form_view" model="ir.ui.view">
            <field name="name">doctors form view</field>
            <field name="model">hms.doctors</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="first_name"/>
                        <field name="last_name"/>
                        <field name="image" widget="image"/>
                    </group>
                </form>
            </field>


        </record>
        <record id="hms_doctors_action" model="ir.actions.act_window">
            <field name="name">doctors</field>
            <field name="res_model">hms.doctors</field>
        </record>

        <menuitem id="hms_doctors_menu"
                  name="doctors"
                  parent="hms_root"
                  action="hms_doctors_action"
                  groups="hms_manager_group"/>




    </data>


</odoo>